{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:default",
    "opener:default",
    "shell:allow-execute",
    "dialog:default",
    "store:default",
    "http:default",
    {
      "identifier": "shell:default",
      "allow": [{ "args": true, "cmd": "**", "name": "**" }]
    }
  ]
}
